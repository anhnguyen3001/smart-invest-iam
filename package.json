  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prestart:prod": "yarn build",
    "postinstall": "yarn prestart:prod",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:watch": "jest --watch --config ./test/jest-config.js",
    "test:cov": "jest --coverage --config ./test/jest-config.js",
    "prepare": "husky install",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "yarn typeorm migration:create -n",
    "migration:up": "yarn typeorm migration:run",
    "migration:down": "yarn typeorm migration:revert"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.1",
