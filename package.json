    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^6.6.6",
    "husky": "^7.0.0",
    "jest": "^27.2.5",
    "lint-staged": "^12.3.6",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.2.4",
