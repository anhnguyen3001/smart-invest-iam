    "passport": "^0.5.2",
    "passport-facebook-token-nest": "^3.3.2",
    "passport-google-token": "^0.1.2",
    "passport-jwt": "^4.0.0",
    "prom-client": "^14.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "speakeasy": "^2.0.0",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.45",
    "typeorm-naming-strategies": "^4.0.0"
